<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:padding="30dip" >

    <Button
        android:id="@+id/me_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/rank_button"
        android:layout_alignRight="@+id/camera_button"
        android:layout_centerVertical="true"
        android:text="@string/me_lable" />

	<Button
	    android:id="@+id/rank_button"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/camera_button"
	    android:layout_alignRight="@+id/camera_button"
	    android:layout_below="@+id/me_button"
	    android:text="Rank" />

	<Button
	    android:id="@+id/camera_button"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_below="@+id/rank_button"
	    android:layout_centerHorizontal="true"
	    android:text="Upload Photo" />

	<TextView
	    android:id="@+id/textView1"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_alignParentTop="true"
	    android:layout_centerHorizontal="true"
	    android:text="@string/main_title"
	    android:textSize="60sp" />

	<Button
	    android:id="@+id/signin_button_main_test"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/camera_button"
	    android:layout_alignRight="@+id/camera_button"
	    android:layout_below="@+id/camera_button"
	    android:text="SignIn_Test" />

	<ImageView
	    android:id="@+id/main_init_imagebutton"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"/>

</RelativeLayout>